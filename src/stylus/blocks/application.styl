.application
  background-color #fff
  color #2374a3
  position relative
  min-height 1254px
  &:before
    content ""
    background url(../images/properties-left-bg-compressor.png) no-repeat
    min-height 1254px
    width 55%
    position absolute
    top 0
    left 0
    background-size 69% 100%
    background-color #fff
  &:after
    content ""
    background url(../images/properties-right-bg-compressor.png) no-repeat
    min-height 1254px
    width 28%
    position absolute
    top 0
    right  0
    background-size 100% 100%
    background-color #fff
  @media (min-width: 875px) and (max-width: 1170px)
    &:before
      width 40%
  @media (max-width: 874px)
    &:before
      width 50%
  @media (max-width: 874px)
    min-height 1700px


  &__title
    color #000
    font-family 'Source Sans Pro', sans-serif
    text-align center
    font-weight 600
    font-size 55px
    position absolute
    top 0
    left 0
    right 0
    z-index 99
    @media (max-width: 874px)
      font-size 28px
      top -22px


  &__title-desc
    color #000
    border solid #000
    font-weight 400
    border-width 0 3px
    border-collapse collapse
    vertical-align middle
    width 100%
    max-width 950px
    margin 120px auto 0 auto
    z-index 9
    max-width 950px
    position absolute
    left 0
    right 0
    *display inline /* для IE7-, чтоб ему( */
    *margin-right .25em /* туда же, чтоб пробелы не "съедал" */
    & td
      font-size 22px
      text-align center
      padding 15px 0
      line-height 1.9em
    & tr:before, & tr:after
      display table-cell
      content '\a0'
      width 1.25em
      border-bottom 3px solid #000
    & tr:first-child:before, & tr:first-child:after
      border-top 3px solid #000

  &__content
    width 100%

  &__content-left
    width 50%
    float left
    background-color #eff7fc
    position relative
    &-inner
      position absolute
      top 312px
      width 100%
      text-align right


  &__content-title
    font-weight 500
    font-size 33px
    text-transform uppercase
    border-bottom 1px solid
    padding-bottom 10px
    margin-right 11%

  &__properties
    list-style none
    top 500px
    position relative
    margin 0
    z-index 9
    &-item
      font-weight 400
      font-size 20px
      position absolute
      border-bottom 1px solid
      padding-bottom 9px
      &:before
        content ""
        width 125px
        height 125px
        display inline-block
        margin -65px 24px -65px -15px
        -webkit-box-shadow 0px 10px 141px 3px rgba(153,208,242,0.58)
        -moz-box-shadow 0px 10px 141px 3px rgba(153,208,242,0.58)
        box-shadow 0px 10px 141px 3px rgba(153,208,242,0.58)
        border-radius 75px
        @media (max-width: 414px)
          margin -82px 30px 0px -17px
    &-item--1
      left 26%
      top -37px
      &:before
        //background url(../images/base64/properties-item-ico/properties-item-ico1.png) no-repeat
        background-image url("data:../images/base64/properties-item-ico/properties-item-ico1.png;base64,")
        background-size contain
      @media (min-width: 875px) and (max-width: 1170px)
        left 14%
      @media (max-width: 874px)
        left 13%
    &-item--2
      left 32%
      top 110px
      &:before
        //background url(../images/base64/properties-item-ico/properties-item-ico2.png) no-repeat
        background-image url("data:../images/base64/properties-item-ico/properties-item-ico2.png;base64,")
        background-size contain
      @media (min-width: 875px) and (max-width: 1170px)
        left 20%
      @media (max-width: 874px)
        left 19%
        top 110px
      @media (max-width: 414px)
        top 140px
    &-item--3
      left 35%
      top 276px
      &:before
        //background url(../images/base64/properties-item-ico/properties-item-ico3.png) no-repeat
        background-image url("data:../images/base64/properties-item-ico/properties-item-ico3.png;base64,")
        background-size contain
      @media (min-width: 875px) and (max-width: 1170px)
        left 22%
      @media (max-width: 874px)
        left 25%
      @media (max-width: 414px)
        top 290px
    &-item--4
      left 33%
      top 449px
      &:before
        //background url(../images/base64/properties-item-ico/properties-item-ico4.png) no-repeat
        background-image url("data:../images/base64/properties-item-ico/properties-item-ico4.png;base64,")
        background-size contain
      @media (min-width: 875px) and (max-width: 1170px)
        left 20%
      @media (max-width: 874px)
        left 18%
      @media (max-width: 414px)
        top 465px
    &-item--5
      left 26%
      top 595px
      &:before
        //background url(../images/base64/properties-item-ico/properties-item-ico5.png) no-repeat
        background-image url("data:../images/base64/properties-item-ico/properties-item-ico5.png;base64,")
        background-size contain
      @media (min-width: 875px) and (max-width: 1170px)
        left 16%
      @media (max-width: 874px)
        left 12%
      @media (max-width: 414px)
        top 650px

  &__content-right
    width 50%
    float right
    background-color #eff7fc
    @media (max-width: 874px)
      width 100%
      float none

    &-inner
      position relative
      top 317px
      background-color #eff7fc
      max-width 412px
      margin-left 93px
      @media (min-width: 875px) and (max-width: 1170px)
        top 327px
        background-color #eff7fc
        max-width 370px
        margin-left 50px
      @media (max-width: 874px)
        top 1250px
        background-color #eff7fc
        margin 0 auto
      @media (max-width: 414px)
        max-width 99%


.application__title-desc
  @media (min-width: 875px) and (max-width: 1170px)
    max-width 820px
  @media (max-width: 874px)
    font-size 15px
    max-width 310px
    margin 60px auto 0 auto

.application__title-desc td
  @media (max-width: 874px)
    font-size: 15px;