.header
  max-width 1386px
  margin 0 auto
  position relative

  &__wrapper
    background: url(../images/head_line.png) no-repeat;
    background-size cover
    min-height 145px

  &__contacts
    float right
    border-top 2px solid #fff
    width 100%
    position absolute
    padding-top 10px
    top 75px
    @media (max-width: 768px)
      padding-top 7px
      border none
      max-width 200px

  &__contacts-inner
    float right
    max-width 600px
    @media (max-width: 800px)
      float none
      max-width 200px
      margin 0 auto

  &__phone
    font-size 17px
    line-height 1.5
    text-align left
    color #fff
    text-decoration none
    font-weight 300
    float left
    padding-right 16px
    display block
    @media (max-width: 768px)
      float left
      margin-left 12px!important
    //&:before
    //  content ""
    //  display inline-block
    //  float left
    //  width 20px
    //  height 20px
    //  margin 2px 10px 0 0
    //  background-image url("data:../images/base64/header-ico/phone-ico.png;base64,")


  &__mail
    font-weight 300
    color #fff
    text-align right
    text-decoration none
    line-height 1.3em
    padding-right 0!important
    display block!important
    font-size 19px
    margin 0 0 0 0
    float left
    //&:before
    //  content ""
    //  display block
    //  float left
    //  width 20px
    //  height 18px
    //  margin 4px 6px 0 13px
    //  background-repeat no-repeat
    //  background-image url("data:../images/base64/header-ico/mail-ico.png;base64,")
    //  @media (max-width: 768px)
    //    margin 4px 6px 0 10px
    @media (max-width: 800px)
      display inline-block!important
      padding-top 0


  &__desc
    color #fff
    font-size 15px
    letter-spacing -0.05em
    font-weight 300
    text-align left
    float left
    text-transform uppercase
    margin-top 16px
    @media (max-width: 768px)
      display none

  &__item
    top 12px
    &--left
      left 0
      position absolute
      z-index 1
      @media (max-width: 768px)
        max-width 200px
        width 100%
        right 0
        left unset

    &--right
      right 0
      position absolute
      z-index 1
      width 76%
      margin-top -5px
      @media (min-width: 993px) and (max-width: 1170px)
        max-width 95%
      @media (max-width: 768px)
        top 0
        left 0
        margin 0 auto
        right unset
        width 100%
        text-align center
        margin 0 auto

    &--desc
      left 25%
      margin 0 auto
      position absolute
      z-index 1
      margin-top 45px
      @media (max-width: 890px)
        display none

  &__icon-close
    display none
    @media (max-width: 768px)
      width 50px
      height 50px
      background-size 70px
      background-repeat no-repeat
      background-image url("data:image/png;base64,")
      display none
      cursor pointer
      padding-left 10px
      padding-top 28px
      padding-bottom 6px
      position absolute
      top 20px
      z-index 999

  &__icon-menu
    display none
    @media (max-width: 768px)
      width 50px
      height 50px
      background-size 50px
      background-repeat no-repeat
      background-image url("data:image/png;base64,")
      display block
      color #232323
      cursor pointer
      font-family 'Open Sans', sans-serif
      font-size 20px
      padding-bottom 25px
      padding-left 25px
      padding-top 14px
      text-decoration none
      text-transform uppercase
      right 15px
      top 80px
      position absolute
      z-index 999


