.button-main-form
  width 100%
  max-width 200px
  color #fff
  border none
  outline none
  background-color #2c3039
  padding 10px 10px


  &:hover
    cursor pointer
    background #404652
    -webkit-transition-property background
    -webkit-transition-duration 0.3s
    -webkit-transition-timing-function ease
